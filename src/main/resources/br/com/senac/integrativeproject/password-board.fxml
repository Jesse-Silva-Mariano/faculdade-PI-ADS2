<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="122.0" prefWidth="256.0" style="-fx-background-color: rgb(48,181,135);" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.senac.integrativeproject.ProjectController">
   <children>
      <Pane layoutX="14.0" layoutY="14.0" prefHeight="94.0" prefWidth="230.0" style="-fx-background-color: white; -fx-background-radius: 20;">
         <children>
            <Label layoutX="19.0" layoutY="4.0" text="Senha do Gestor:">
               <font>
                  <Font name="Arial Bold" size="23.0" />
               </font>
            </Label>
            <PasswordField fx:id="passwordFieldUser" layoutX="35.0" layoutY="35.0" onKeyPressed="#handleKeyRegister" prefHeight="30.0" prefWidth="159.0" promptText="********" style="-fx-background-color: #ebebeb; -fx-background-radius: 5;">
               <font>
                  <Font name="Arial" size="14.0" />
               </font>
            </PasswordField>
            <Label fx:id="labelConfirmPassword" layoutX="84.0" layoutY="72.0" onMousePressed="#handleLabelConfirmPassword" stylesheets="@../../../../styles/LabelCadatroEffect.css" text="Confirmar">
               <font>
                  <Font name="Arial" size="14.0" />
               </font>
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor>
            </Label>
         </children>
      </Pane>
   </children>
</AnchorPane>
